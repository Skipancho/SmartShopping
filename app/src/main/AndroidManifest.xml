<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartshopping">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/simple_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WithdrawalActivity" />
        <activity android:name=".ChangePwActivity" />
        <activity android:name=".FindPwActivity" />
        <activity android:name=".FindIdActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MonthlyStatisticsActivity" />
        <activity android:name=".StatisticsActivity" />
        <activity android:name=".UserInfoActivity" />
        <activity android:name=".PasswordPop" />
        <activity android:name=".ProductPop" />
        <activity android:name=".ReviewActivity" />
        <activity android:name=".PurchaseListInqury" />
        <activity android:name=".MyPageActivity" />
        <activity android:name=".LoginActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>